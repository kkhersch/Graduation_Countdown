<h1> All Students </h1>
<table>
	<thead>
		<tr>
			<td>Credits </td>
			<td>Declared Major </td>
			<td>Days until graduation </td>
		</tr>
	</thead>
	<tbody>
		<% @student.each do |students| %>
		<tr>
			<td><%= students.student_name %></td>
			<td><%= students.credits %></td>
			<td><%= students.declared_major %></td>
			<td><%= students.caluclate_time_until_graduation %></td>
		</tr>
		<% end %>
	</tbody>

	<%= link_to 'New', new_student_path %>
</table>