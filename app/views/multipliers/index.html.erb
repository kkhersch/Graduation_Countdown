<h1>All Multipliers</h1>
<table>
  <thead>
    <tr>
      <td>Name</td>
      <td>Value</td>
    </tr>
  </thead>
  <tbody>
    <% @multiplier.each do |multiplier| %>
      <tr class="multiplier-row" id="<%= dom_id(multiplier) %>">
        <td class="name"><%= multiplier.multiplier_name %></td>
        <td class = "multiplier_value"><%= multiplier.multiplier_value %></td>
      </tr>
    <% end %>
  </tbody>
  <%= link_to 'New', new_multiplier_path %>
</table>
